{
  "name": "@pydantic/mcp-run-python",
  "version": "0.0.16",
  "license": "MIT",
  "nodeModulesDir": "auto",
  "exports": {
    ".": "./src/main.ts"
  },
  "tasks": {
    "dev": "deno run -N -R=node_modules -W=node_modules src/main.ts streamable_http --port 3001",
    "start": "deno run --allow-net --allow-read --allow-write --node-modules-dir=auto src/main.ts streamable_http --port 3001"
  },
  "imports": {
    "@modelcontextprotocol/sdk": "npm:@modelcontextprotocol/sdk@^1.15.1",
    "@std/cli": "jsr:@std/cli@^1.0.15",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@std/fs": "jsr:@std/fs@^1.0.8",
    // do NOT upgrade above this version until there is a workaround for https://github.com/pyodide/pyodide/pull/5621
    "pyodide": "npm:pyodide@0.27.6",
    "zod": "npm:zod@^3.24.2"
  },
  "fmt": {
    "lineWidth": 120,
    "semiColons": false,
    "singleQuote": true,
    "include": ["."]
  }
}
